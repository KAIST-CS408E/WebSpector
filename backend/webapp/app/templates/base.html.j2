<!DOCTYPE html>
<html style="background-color: #efefef;">

<head>
  <title>{% block title %}{% endblock %}</title>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" href="{{ url_for('static', filename='css/uikit.min.css') }}">
  <link rel="stylesheet" href="{{ url_for('static', filename='css/Nwagon.css') }}" type="text/css">
  <script src="{{ url_for('static', filename='js/Nwagon.js') }}"></script>
  <script src="https://code.jquery.com/jquery-3.3.1.min.js" integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8=" crossorigin="anonymous"></script>
</head>

<body>
  {% block content %} {% endblock %}
  <script src="{{ url_for('static', filename='js/uikit.min.js') }}" charset="utf-8"></script>
  <script src="{{ url_for('static', filename='js/uikit-icons.min.js') }}" charset="utf-8"></script>
  {% block script %} {% endblock %}
</body>
</html>
